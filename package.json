{
	"name": "directus-extension-schema-sync",
	"description": "Sync schema and data betwreen Directus instances",
	"icon": "sync_alt",
	"version": "3.0.6",
	"repository": {
		"type": "git",
		"url": "https://github.com/bcc-code/directus-schema-sync.git"
	},
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-hook",
		"directus-schema",
		"directus-data"
	],
	"type": "module",
	"directus:extension": {
		"type": "hook",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^11.0.0"
	},
	"files": [
		"dist",
		"install",
		"README.md"
	],
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"pre-test": "tsc -p tsconfig.test.json",
		"test": "npm run pre-test && node --test dist-test/",
		"create-version": "node ./scripts/version.cjs",
		"release-channel": "node ./scripts/release-channel.js"
	},
	"devDependencies": {
		"@directus/api": "^22.0.0",
		"@directus/extensions": "^3.0.0",
		"@directus/extensions-sdk": "^12.1.3",
		"@directus/types": "^13.0.0",
		"@types/keyv": "^4.2.0",
		"@types/node": "^20.14.15",
		"typescript": "^5.5.4"
	},
	"dependencies": {
		"glob": "^10.4.5"
	}
}
